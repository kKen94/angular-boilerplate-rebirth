{
  "name": "angular-boilerplate",
  "version": "0.0.1",
  "scripts": {
    "start": "cross-env NODE_ENV=dev ng serve --open",
    "build:dev": "cross-env NODE_ENV=dev ng build",
    "build:staging": "cross-env NODE_ENV=staging ng build -c=staging",
    "build:prod": "cross-env NODE_ENV=prod ng build --prod",
    "test:unit": "ng test",
    "test:e2e": "ng e2e",
    "lint": "yarn run lint:code && yarn run lint:style",
    "lint:code": "ng lint",
    "lint:style": "stylelint src/**/*.scss",
    "fix": "yarn run fix:code && yarn run fix:style",
    "fix:code": "prettier --write \"src/**/*.{ts,js,html}\"",
    "fix:code:staged": "pretty-quick --staged",
    "fix:prettier:staged": "pretty-quick --staged",
    "fix:style": "stylelint src/**/*.scss --fix",
    "report:prod": "cross-env NODE_ENV=prod ng build --prod --stats-json",
    "analyze:prod": "yarn run report:prod && yarn run webpack-bundle-analyzer dist/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.1.0",
    "@angular/cdk": "^11.1.0",
    "@angular/common": "~11.1.0",
    "@angular/compiler": "~11.1.0",
    "@angular/core": "~11.1.0",
    "@angular/forms": "~11.1.0",
    "@angular/material": "^11.1.0",
    "@angular/platform-browser": "~11.1.0",
    "@angular/platform-browser-dynamic": "~11.1.0",
    "@angular/router": "~11.1.0",
    "@kken94/ngx-progress": "^2.3.0",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "keycloak-angular": "^8.4.0",
    "keycloak-js": "^15.0.2",
    "rxjs": "~6.6.0",
    "sweetalert2": "^10.13.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^10.0.0",
    "@angular-devkit/build-angular": "~0.1101.1",
    "@angular-eslint/builder": "1.1.0",
    "@angular-eslint/eslint-plugin": "1.1.0",
    "@angular-eslint/eslint-plugin-template": "1.1.0",
    "@angular-eslint/schematics": "1.1.0",
    "@angular-eslint/template-parser": "1.1.0",
    "@angular/cli": "~11.1.1",
    "@angular/compiler-cli": "~11.1.0",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@commitlint/prompt": "^11.0.0",
    "@fullhuman/postcss-purgecss": "^4.0.0",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "4.14.1",
    "@typescript-eslint/parser": "4.14.1",
    "autoprefixer": "^10.2.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jasmine": "^4.1.1",
    "eslint-plugin-jsdoc": "31.3.3",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-rxjs": "^3.0.0",
    "husky": "^4.3.7",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.2.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "lint-staged": "^10.5.3",
    "postcss": "^8.2.3",
    "postcss-custom-properties": "^11.0.0",
    "postcss-import": "^14.0.0",
    "postcss-loader": "^4.1.0",
    "postcss-nested": "^5.0.3",
    "postcss-scss": "^3.0.4",
    "prettier": "^2.2.1",
    "prettier-plugin-tailwind": "^2.2.8",
    "pretty-quick": "^3.1.0",
    "protractor": "~7.0.0",
    "stylelint": "^13.8.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-sass-guidelines": "^7.1.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.18.0",
    "tailwindcss": "^2.0.2",
    "ts-node": "~8.3.0",
    "typescript": "~4.0.2",
    "webpack-bundle-analyzer": "^4.3.0"
  }
}
