{
  "name": "angular-boilerplate",
  "version": "0.0.1",
  "scripts": {
    "start": "cross-env NODE_ENV=dev ng serve --open",
    "build:dev": "cross-env NODE_ENV=dev ng build",
    "build:staging": "cross-env NODE_ENV=staging ng build --configuration staging",
    "build:prod": "cross-env NODE_ENV=prod ng build --configuration production",
    "test:unit": "ng test",
    "test:e2e": "ng e2e",
    "lint": "yarn run lint:code && yarn run lint:style",
    "lint:code": "ng lint",
    "lint:style": "stylelint \"**/*.scss\"",
    "fix": "yarn run fix:eslint && yarn run fix:style",
    "fix:code": "ng lint --fix",
    "fix:style": "stylelint \"**/*.scss\" --fix",
    "report:prod": "cross-env NODE_ENV=prod ng build --configuration production --stats-json",
    "analyze:prod": "yarn run report:prod && yarn run webpack-bundle-analyzer dist/stats.json",
    "prepare": "husky install",
    "commit": "cz"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.1.1",
    "@angular/cdk": "^13.1.1",
    "@angular/common": "~13.1.1",
    "@angular/compiler": "~13.1.1",
    "@angular/core": "~13.1.1",
    "@angular/forms": "~13.1.1",
    "@angular/material": "^13.1.1",
    "@angular/platform-browser": "~13.1.1",
    "@angular/platform-browser-dynamic": "~13.1.1",
    "@angular/router": "~13.1.1",
    "@kken94/ngx-progress": "^2.3.0",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "keycloak-angular": "^9.0.0",
    "keycloak-js": "^16.1.0",
    "rxjs": "~7.4.0",
    "sweetalert2": "^11.3.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^13.0.0",
    "@angular-devkit/build-angular": "^13.1.2",
    "@angular-eslint/builder": "^13.0.1",
    "@angular-eslint/eslint-plugin": "^13.0.1",
    "@angular-eslint/eslint-plugin-template": "^13.0.1",
    "@angular-eslint/schematics": "13.0.1",
    "@angular-eslint/template-parser": "^13.0.1",
    "@angular/cli": "~13.1.2",
    "@angular/compiler-cli": "~13.1.1",
    "@commitlint/cli": "^16.0.2",
    "@commitlint/config-conventional": "^16.0.0",
    "@commitlint/prompt": "^16.0.0",
    "@fullhuman/postcss-purgecss": "^4.0.0",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "autoprefixer": "^10.2.0",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jasmine": "^4.1.1",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-rxjs": "^5.0.0",
    "husky": "^7.0.0",
    "jasmine-core": "^4.0.0",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "~6.3.9",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "lint-staged": "^12.1.2",
    "postcss": "^8.2.3",
    "postcss-custom-properties": "^12.0.0",
    "postcss-import": "^14.0.0",
    "postcss-loader": "^6.2.1",
    "postcss-nested": "^5.0.3",
    "postcss-scss": "^4.0.2",
    "prettier": "^2.2.1",
    "protractor": "~7.0.0",
    "stylelint": "^14.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard-scss": "^3.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.0.1",
    "tailwindcss": "^3.0.0",
    "ts-node": "~8.3.0",
    "typescript": "~4.5.2",
    "webpack-bundle-analyzer": "^4.3.0"
  }
}
